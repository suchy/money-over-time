{
  "name": "money-over-time",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production remix-serve build",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./src/web/styles/app.css -o ./src/web/app/styles/app.css",
    "build": "cross-env NODE_ENV=production remix build",
    "build:css": "tailwindcss -m -i ./src/web/styles/app.css -o ./src/web/app/styles/app.css",
    "lint": "eslint ./src",
    "prepare": "husky install",
    "postinstall": "remix setup node  && metronome setup",
    "infrastructure:start": "docker-compose up -d",
    "infrastructure:stop": "docker-compose down"
  },
  "dependencies": {
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.1",
    "firebase-admin": "^10.2.0",
    "focus-trap-react": "^9.0.2",
    "lodash": "^4.17.21",
    "mathjs": "^11.0.0",
    "metronome-sh": "^1.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.1.10",
    "remix": "^1.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.5",
    "@types/jest": "^28.1.6",
    "@types/mathjs": "^9.4.2",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "sideEffects": false,
  "engines": {
    "node": "16",
    "npm": "8"
  }
}
